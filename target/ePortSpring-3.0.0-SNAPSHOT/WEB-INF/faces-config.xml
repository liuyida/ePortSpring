<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
	<managed-bean>
		<managed-bean-name>indexPage</managed-bean-name>
		<managed-bean-class>com.xmhuanyu.eport.page.IndexPage</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>metalOrderPage</managed-bean-name>
		<managed-bean-class>com.xmhuanyu.eport.page.MetalOrderPage</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>menuTreeBean</managed-bean-name>
		<managed-bean-class>com.xmhuanyu.eport.page.MenuTreeBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>pages/index.xhtml</display-name>
		<from-view-id>/pages/index.xhtml</from-view-id>
		<navigation-case>
		    <from-outcome>metalOrderPage</from-outcome>
			<to-view-id>/pages/metalOrderPage.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
